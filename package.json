{
  "name": "rest-project",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:up": "docker-compose --profile local-env up --build",
    "docker:down": "docker-compose --profile local-env down",
    "docker:logs": "docker-compose --profile local-env logs -f",
    "docker:logs:api": "docker-compose --profile local-env logs -f api",
    "docker:logs:db": "docker-compose --profile local-env logs -f pg",
    "docker:logs:redis": "docker-compose --profile local-env logs -f redis",
    "docker:clean": "docker-compose --profile local-env down -v --remove-orphans",
    "docker:rebuild": "docker-compose --profile local-env down && docker-compose --profile local-env up --build",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:migrate:deploy": "npx prisma migrate deploy",
    "db:reset": "npx prisma migrate reset",
    "db:seed": "npx prisma db seed",
    "db:studio": "npx prisma studio",
    "db:init": "npm run db:generate && npm run db:migrate",
    "docker:db:generate": "docker-compose --profile local-env exec api npx prisma generate",
    "docker:db:migrate": "docker-compose --profile local-env exec api npx prisma migrate dev",
    "docker:db:migrate:deploy": "docker-compose --profile local-env exec api npx prisma migrate deploy",
    "docker:db:reset": "docker-compose --profile local-env exec api npx prisma migrate reset",
    "docker:db:seed": "docker-compose --profile local-env exec api npx prisma db seed",
    "docker:db:studio": "docker-compose --profile local-env exec api npx prisma studio",
    "docker:db:init": "npm run docker:db:generate && npm run docker:db:migrate && npm run docker:db:seed",
    "docker:setup": "npm run setup:env && npm run docker:up && npm run docker:db:init",
    "setup:env": "cp environment.config.example .env",
    "setup": "npm install && npm run setup:env && npm run db:init"
  },
  "prisma": {
    "seed": "ts-node src/database/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@prisma/client": "^6.11.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^6.11.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}